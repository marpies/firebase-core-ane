<extension xmlns="http://ns.adobe.com/air/extension/20.0">
  <id>com.marpies.ane.firebase.core</id>
  <versionNumber>1.0.0</versionNumber>
  <platforms>
    <platform name="iPhone-ARM">
        <applicationDeployment>
            <nativeLibrary>libFirebaseCore.a</nativeLibrary>
            <initializer>FirebaseCoreInitializer</initializer>
            <finalizer>FirebaseCoreFinalizer</finalizer>
        </applicationDeployment>
    </platform>

    <platform name="Android-ARM">
        <applicationDeployment>
            <nativeLibrary>libFirebaseCore.jar</nativeLibrary>
            <initializer>com.marpies.ane.firebase.core.FirebaseCoreExtension</initializer>
            <finalizer>com.marpies.ane.firebase.core.FirebaseCoreExtension</finalizer>
        </applicationDeployment>
    </platform>

    <platform name="Android-x86">
        <applicationDeployment>
            <nativeLibrary>libFirebaseCore.jar</nativeLibrary>
            <initializer>com.marpies.ane.firebase.core.FirebaseCoreExtension</initializer>
            <finalizer>com.marpies.ane.firebase.core.FirebaseCoreExtension</finalizer>
        </applicationDeployment>
    </platform>

    <platform name="default">
        <applicationDeployment/>
    </platform>
  </platforms>
</extension>
